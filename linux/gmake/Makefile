# GNU Make solution makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=release_x32
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release_x32)
  roc_config = release_x32
endif

PROJECTS := roc

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

roc:
ifneq (,$(roc_config))
	@echo "==== Building roc ($(roc_config)) ===="
	@${MAKE} --no-print-directory -C . -f roc.make config=$(roc_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f roc.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release_x32"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   roc"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
